{
  "name": "epa-avert-client",
  "version": "4.0.0",
  "description": "Front end of the AVERT web application.",
  "homepage": ".",
  "author": "USEPA (https://www.epa.gov)",
  "contributors": [
    "Bryan Neva <bryan.neva@erg.com>",
    "Courtney Myers <courtney.myers@erg.com>"
  ],
  "license": "CC0-1.0",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "REACT_APP_URL=https://avert-dev.app.cloud.gov npm run build",
    "build": "react-scripts build && ./sync-build-with-public.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "prepare": "cd .. && husky install client/.husky"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/react-redux": "7.1.23",
    "@types/stats-lite": "2.2.0",
    "cypress": "9.5.2",
    "eslint-plugin-cypress": "2.12.1",
    "husky": "7.0.4",
    "lint-staged": "12.3.6",
    "prettier": "2.6.0",
    "react-scripts": "5.0.0",
    "tailwindcss": "3.3.1",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@headlessui/react": "1.7.13",
    "@heroicons/react": "2.0.17",
    "@reach/tabs": "0.16.4",
    "@uswds/uswds": "3.3.0",
    "highcharts": "10.0.0",
    "highcharts-react-official": "3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-select": "5.7.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "stats-lite": "2.2.0",
    "web-vitals": "2.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
