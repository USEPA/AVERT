{
  "name": "epa-avert-client",
  "version": "4.3.0",
  "description": "Front end of the AVERT web application.",
  "homepage": ".",
  "author": "USEPA (https://www.epa.gov)",
  "contributors": [
    "Bryan Neva <bryan.neva@erg.com>",
    "Courtney Myers <courtney.myers@erg.com>"
  ],
  "license": "CC0-1.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "VITE_APP_URL=http://localhost:3001 vite",
    "build:dev": "tsc && vite build --mode development && ./sync-build-with-public.sh",
    "build:prod": "tsc && vite build --mode production && ./sync-build-with-public.sh",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "cypress": "cypress open",
    "prepare": "cd .. && husky client/.husky"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.1",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/stats-lite": "2.2.2",
    "@typescript-eslint/eslint-plugin": "7.3.0",
    "@typescript-eslint/parser": "7.3.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.18",
    "cypress": "13.7.0",
    "eslint": "8.57.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.37",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.12",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "vite": "5.1.6"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "@reach/tabs": "0.18.0",
    "@uswds/uswds": "3.8.0",
    "highcharts": "11.4.0",
    "highcharts-react-official": "3.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "8.1.3",
    "react-select": "5.8.0",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0",
    "stats-lite": "2.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": "prettier --write"
  }
}
