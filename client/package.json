{
  "name": "epa-avert-client",
  "version": "4.2.0",
  "description": "Front end of the AVERT web application.",
  "homepage": ".",
  "author": "USEPA (https://www.epa.gov)",
  "contributors": [
    "Bryan Neva <bryan.neva@erg.com>",
    "Courtney Myers <courtney.myers@erg.com>"
  ],
  "license": "CC0-1.0",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "REACT_APP_URL=https://avert-dev.app.cloud.gov npm run build",
    "build": "react-scripts build && ./sync-build-with-public.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "prepare": "cd .. && husky install client/.husky"
  },
  "devDependencies": {
    "@testing-library/cypress": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/stats-lite": "2.2.0",
    "cypress": "12.9.0",
    "eslint-plugin-cypress": "2.13.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "react-scripts": "5.0.1",
    "tailwindcss": "3.3.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@headlessui/react": "1.7.13",
    "@heroicons/react": "2.0.17",
    "@reach/tabs": "0.18.0",
    "@uswds/uswds": "3.4.1",
    "highcharts": "10.3.3",
    "highcharts-react-official": "3.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "8.0.5",
    "react-select": "5.7.2",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "stats-lite": "2.2.0",
    "web-vitals": "3.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
