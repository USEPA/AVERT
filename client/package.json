{
  "name": "epa-avert-client",
  "version": "2.0.0",
  "description": "Front end of the AVERT web application.",
  "homepage": ".",
  "author": "USEPA (https://www.epa.gov)",
  "contributors": [
    "Bryan Neva <bryan.neva@erg.com>",
    "Courtney Myers <courtney.myers@erg.com>"
  ],
  "license": "CC0-1.0",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "REACT_APP_URL=https://avert-dev.app.cloud.gov npm run build",
    "build": "react-scripts build && ./sync-build-with-public.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "prepare": "cd .. && husky install client/.husky"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.9",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/stats-lite": "2.2.0",
    "cypress": "8.3.1",
    "eslint-plugin-cypress": "2.11.3",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "react-scripts": "4.0.3",
    "typescript": "4.4.2"
  },
  "dependencies": {
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@reach/tabs": "0.16.1",
    "core-js": "3.16.4",
    "highcharts": "9.2.2",
    "highcharts-react-official": "3.0.0",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0",
    "stats-lite": "2.2.0",
    "web-vitals": "2.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
