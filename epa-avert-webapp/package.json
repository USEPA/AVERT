{
  "name": "epa-avert-webapp",
  "version": "1.15.0",
  "private": true,
  "description": "EPA's Avoided Emissions and Generation Tools (AVERT) is a free tool that estimates the emissions benefits of energy efficiency and renewable energy policies and programs.",
  "author": "Eastern Research Group, Inc.",
  "contributors": [
    "Bryan Neva <bryan.neva@erg.com>",
    "Courtney Myers <courtney.myers@erg.com>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eastern-Research-Group/EPA-AVERT.git"
  },
  "bugs": {
    "url": "https://github.com/Eastern-Research-Group/EPA-AVERT/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "react-scripts": "1.1.4",
    "redux-mock-store": "1.5.3"
  },
  "dependencies": {
    "blob": "0.0.4",
    "file-saver": "1.3.8",
    "flow-bin": "0.78.0",
    "highcharts-exporting": "0.1.7",
    "husky": "0.14.3",
    "json2csv": "4.2.1",
    "lint-staged": "7.2.0",
    "prettier": "1.14.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-highcharts": "16.0.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "stats-lite": "2.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build:pcf": "REACT_APP_URL=https://avert-dev.cfapps.io npm run build",
    "build:dev": "REACT_APP_URL=https://avert-dev.app.cloud.gov npm run build",
    "build": "react-scripts build && ./sync-build-with-public.sh",
    "test": "react-scripts test --verbose",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "flow": "flow"
  }
}
